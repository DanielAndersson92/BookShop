<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
	  <message-bundle>
	  </message-bundle>
     </application>
    <navigation-rule>
        <from-view-id>*</from-view-id> 
        <navigation-case>
            <from-outcome>start</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mypage</from-outcome>
            <to-view-id>/jsf/Mypage/mypage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>search</from-outcome>
            <to-view-id>/jsf/Search/search.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>forum</from-outcome>
            <to-view-id>/jsf/Forum/forum.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>about</from-outcome>
            <to-view-id>/jsf/About/about.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
</faces-config>
